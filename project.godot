; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="multiSceneAndPlayer"
run/main_scene="res://Init.tscn"
config/icon="res://icon.png"

[autoload]

G="*res://Classes/G.gd"
Code="*res://Classes/Code.gd"
D="*res://Classes/D.gd"
GameInitForDebug="*res://GameInitForDebug.gd"

[display]

window/size/width=640
window/size/height=360
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "TextEditDecrypted", "TextEditEncrypted", "TextEditSingleCharacterColor" )

[rendering]

environment/default_clear_color=Color( 0.266667, 0.223529, 0.223529, 1 )
environment/default_environment="res://default_env.tres"
